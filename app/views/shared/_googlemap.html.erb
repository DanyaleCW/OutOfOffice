<div id="map-container">
  <div id="map"></div>
</div>

<script src="https://maps.googleapis.com/maps/api/js?key=<%=ENV['GOOGLE_API_KEY']%>&callback=initMap"></script>

<%= form_for :user, url: url_for(controller: 'places', action: 'create') do |f| %>
  <%= f.hidden_field :zip_code %>
  <%= f.submit "Confirm location" %>
<% end %>
